_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="21"/><column index="2" value="98"/><column index="3" value="127"/><column index="4" value="154"/><column index="5" value="167"/><column index="6" value="173"/><column index="7" value="167"/><column index="8" value="202"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sqlite_sequence" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="91"/><column index="2" value="32"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Edit View Player_Category_Rate_Interval_List">-- Fix the View to include Hourly_Rate and Prepay_Hours
-- DROP VIEW IF EXISTS &quot;Player_Selection_List&quot;;

-- CREATE VIEW &quot;Player_Selection_List&quot; AS
SELECT 
    p.Player_Id,
    IFNULL(p.NickName, p.Name) as Name,
    pb.Balance,
    -- These two columns were missing in your old DB:
    c.Rate as Hourly_Rate,
    c.Prepay_Hours as Prepay_Hours,
    CASE WHEN s.Session_Id IS NOT NULL THEN 1 ELSE 0 END as Is_Active
FROM Player p
JOIN Player_Balance pb ON p.Player_Id = pb.Player_Id
JOIN Player_Category_Rate_Interval_List c ON p.Player_Category_Id = c.Player_Category_Id
LEFT JOIN Session s ON p.Player_Id = s.Player_Id AND s.Stop_Epoch IS NULL
ORDER BY p.Name;</sql><sql name="Edit View Player_Selection_List">UPDATE Session SET PokerTable_Id=NULL;
</sql><sql name="Edit View Session_Panel_List">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Session_Panel_List&quot;;
CREATE VIEW &quot;Session_Panel_List&quot; AS
SELECT
    sl.Session_Id, sl.Player_Id, sl.Name, sl.Start_Epoch, sl.Stop_Epoch, sl.PokerTable_Id, sl.Seat_Number, sl.Is_Prepaid, sl.Prepay_Amount,
    sal.Duration_In_Seconds, sal.Amount, pb.Balance, sal.Rate
FROM Session_List as sl
JOIN Session_Amount_List as sal ON sl.Session_Id == sal.Session_Id
JOIN Player_Balance as pb ON sl.Player_Id == pb.Player_Id
ORDER BY CASE WHEN sl.Stop_Epoch IS NULL THEN 0 ELSE 1 END, sal.Rate, sl.Name</sql><sql name="Edit View Session_Amount_List">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Session_Amount_List&quot;;
CREATE VIEW &quot;Session_Amount_List&quot; AS
SELECT
    sr.Session_Id, sr.Session_Start_Epoch, sr.Session_Stop_Epoch,
    CASE WHEN sr.Session_Stop_Epoch IS NULL THEN NULL ELSE sr.Session_Stop_Epoch - sr.Session_Start_Epoch END as Duration_In_Seconds,
    sr.Player_Id, sr.Name, sr.Category, sr.Rate, sr.Rate_Description, sr.Is_Prepaid, sr.Prepay_Amount,
    CASE
        WHEN sr.Is_Prepaid = 1 THEN sr.Prepay_Amount
        WHEN sr.Session_Stop_Epoch IS NULL THEN NULL
        ELSE CAST(round(MAX(sr.Session_Stop_Epoch - sr.Session_Start_Epoch, 0) * sr.Rate  / 3600.0 ) AS INTEGER)
    END as Amount
FROM Session_Rate_List as sr</sql><sql name="Edit View Session_Panel_List">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Session_Panel_List&quot;;
CREATE VIEW &quot;Session_Panel_List&quot; AS
SELECT
    sl.Session_Id, sl.Player_Id, sl.Name, sl.Start_Epoch, sl.Stop_Epoch, sl.PokerTable_Id, sl.Seat_Number, sl.Is_Prepaid, sl.Prepay_Amount,
    sal.Duration_In_Seconds, sal.Amount, pb.Balance, sal.Rate
FROM Session_List as sl
JOIN Session_Amount_List as sal ON sl.Session_Id == sal.Session_Id
JOIN Player_Balance as pb ON sl.Player_Id == pb.Player_Id
ORDER BY CASE WHEN sl.Stop_Epoch IS NULL THEN 0 ELSE 1 END, sal.Rate, sl.Name</sql><sql name="Edit View Player_Selection_List">-- 1. Create the single-row System_State table
CREATE TABLE IF NOT EXISTS System_State (
    Id INTEGER PRIMARY KEY CHECK (Id = 1),
    Is_Club_Open INTEGER NOT NULL DEFAULT 0,
    Club_Start_Epoch INTEGER,
    Default_Session_Hour INTEGER NOT NULL DEFAULT 19,
    Default_Session_Minute INTEGER NOT NULL DEFAULT 30,
    Floor_Manager_Player_Id INTEGER,
    Floor_Manager_Table_Id INTEGER,
    Floor_Manager_Seat_Number INTEGER
);

-- 2. Insert the foundational state (INSERT OR IGNORE prevents overwriting if it already exists)
INSERT OR IGNORE INTO System_State (
    Id, 
    Is_Club_Open, 
    Club_Start_Epoch, 
    Default_Session_Hour, 
    Default_Session_Minute, 
    Floor_Manager_Player_Id, 
    Floor_Manager_Table_Id, 
    Floor_Manager_Seat_Number
) VALUES (
    1,          -- Enforced ID
    0,          -- Club is currently closed
    NULL,       -- No start time yet
    19,         -- 7 PM
    30,         -- 30 Minutes
    1,          -- Floor Manager is Player 1 (Pony)
    1,          -- Reserved Table 1
    1           -- Reserved Seat 1
);</sql><current_tab id="5"/></tab_sql></sqlb_project>

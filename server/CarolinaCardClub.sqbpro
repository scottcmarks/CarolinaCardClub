mn_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="Session_Rate_List" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="80"/><column index="2" value="69"/><column index="3" value="148"/><column index="4" value="145"/><column index="5" value="112"/><column index="6" value="102"/><column index="7" value="88"/><column index="8" value="126"/><column index="9" value="124"/><column index="10" value="38"/><column index="11" value="120"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="Super_Bowl_Players" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="70"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sqlite_sequence" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="91"/><column index="2" value="32"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1"></sql><sql name="Edit View Player_Selection_List">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Player_Selection_List&quot;;
CREATE VIEW &quot;Player_Selection_List&quot; AS SELECT p.Player_Id, IFNULL(p.NickName, p.Name) as Name, IFNULL(b.Balance,0.00) as Balance, start.Most_Recent_Start_Epoch is not NULL AND stop.Most_Recent_Stop_Epoch is NULL as Active FROM Player as p LEFT JOIN Player_Most_Recent_Session_Start_Epoch as start ON p.Player_Id == start.Player_Id LEFT JOIN Player_Most_Recent_Session_Stop_Epoch as stop ON p.Player_Id == stop.Player_Id LEFT JOIN Player_Balance as b ON p.Player_Id == b.Player_Id WHERE p.Player_Id IN (SELECT Player_Id FROM Player_Phone) AND p.Flag IS NULL ORDER BY start.Most_Recent_Start_Epoch IS NULL, p.Player_Category_Id ASC, Name ASC</sql><sql name="Edit View Player_Selection_List*">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Player_Selection_List&quot;;
CREATE VIEW &quot;Player_Selection_List&quot; AS 
SELECT 
  p.Player_Id, 
  IFNULL(p.NickName, p.Name) as Name, 
  IFNULL(b.Balance,0.00) as Balance, 
  start.Most_Recent_Start_Epoch is not NULL AND stop.Most_Recent_Stop_Epoch is NULL as Active 
FROM 
  Player as p 
LEFT JOIN 
  Player_Most_Recent_Session_Start_Epoch as start ON p.Player_Id == start.Player_Id 
LEFT JOIN 
  Player_Most_Recent_Session_Stop_Epoch as stop ON p.Player_Id == stop.Player_Id 
LEFT JOIN 
  Player_Balance as b ON p.Player_Id == b.Player_Id 
WHERE p.Player_Id IN (SELECT Player_Id FROM Player_Phone) AND p.Flag IS NULL 
ORDER BY 
  start.Most_Recent_Start_Epoch IS NULL, 
  start.Most_Recent_Start_Week DESC, 
  p.Player_Category_Id ASC, Name ASC</sql><sql name="Edit View Player_Most_Recent_Session_Start_Epoch*">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Player_Most_Recent_Session_Start_Epoch&quot;;
CREATE VIEW &quot;Player_Most_Recent_Session_Start_Epoch&quot; AS
SELECT
s.Player_Id as Player_Id,
s.Session_Id as Session_Id,
MAX(s.Start_Epoch) as Most_Recent_Start_Epoch,
MAX(s.Start_Epoch)/(60*60*24*7) as Most_Recent_Start_Week
FROM Session as s
GROUP BY s.Player_Id
ORDER BY s.Start_Epoch DESC</sql><current_tab id="2"/></tab_sql></sqlb_project>

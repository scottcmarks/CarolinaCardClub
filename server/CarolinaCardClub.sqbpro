    p.Name as Name,
    -- Use a CASE statement to handle players with no email addresses
    CASE
        WHEN COUNT(DISTINCT ea.Address) = 0 THEN '[]'
        ELSE json_group_array(DISTINCT ea.Address)
    END as EmailAddresses,
    -- Use a CASE statement to handle players with no phone numbers
    CASE
        WHEN COUNT(DISTINCT pn.Number) = 0 THEN '[]'
        ELSE json_group_array(DISTINCT pn.Number)
    END as PhoneNumbers,
    MAX(sb.Player_Id IS NOT NULL) as Super_Bowl,
    p.Flag as Flag
FROM
    Player as p
LEFT JOIN
    Player_Email as pe ON pe.Player_Id = p.Player_Id
LEFT JOIN
    Email_Address as ea ON pe.EmailAddress_Id = ea.EmailAddress_Id
LEFT JOIN
    Player_Phone as pp ON pp.Player_Id = p.Player_Id
LEFT JOIN
    Phone_Number as pn ON pp.PhoneNumber_Id = pn.PhoneNumber_Id
LEFT JOIN
    Super_Bowl_Players as sb ON sb.Player_Id = p.Player_Id
GROUP BY
    p.Player_Id
ORDER BY
    p.Player_Id;</sql><sql name="SQL 2">BEGIN TRANSACTION;

-- Step 1: Temporarily disable foreign key constraints to allow table modification.
PRAGMA foreign_keys=OFF;

-- Step 2: Rename the existing table to a temporary name.
ALTER TABLE Phone_Number RENAME TO Phone_Number_old;

-- Step 3: Create the new table with the desired schema, including the new columns.
CREATE TABLE &quot;Phone_Number&quot; (
    &quot;PhoneNumber_Id&quot;    INTEGER NOT NULL UNIQUE,
    &quot;Number&quot;            TEXT NOT NULL UNIQUE,
    &quot;IsWorking&quot;         INTEGER NOT NULL DEFAULT 1,
    &quot;CanSMS&quot;            INTEGER,
    PRIMARY KEY(&quot;PhoneNumber_Id&quot; AUTOINCREMENT)
);

-- Step 4: Copy the preserved data from the old table into the new one.
-- The IsWorking and CanSMS columns will automatically get their default values (1 and NULL).
INSERT INTO Phone_Number (PhoneNumber_Id, Number)
SELECT PhoneNumber_Id, Number FROM Phone_Number_old;

-- Step 5: Drop the old, temporary table.
DROP TABLE Phone_Number_old;

-- Step 6: Re-enable foreign key constraints.
PRAGMA foreign_keys=ON;

COMMIT;

-- Optional: Clean up the database file to reclaim space.
VACUUM;</sql><sql name="Edit View Email_List">DROP VIEW IF EXISTS &quot;main&quot;.&quot;Email_List&quot;;
